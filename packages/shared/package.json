{
	"name": "@huggingface/shared",
	"version": "0.0.0",
	"description": "Shared utilities for HuggingFace.js",
	"private": true,
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.ts"
		}
	},
	"engines": {
		"node": ">=18"
	},
	"source": "index.ts",
	"scripts": {
		"lint": "eslint --quiet --fix --ext .cjs,.ts .",
		"lint:check": "eslint --ext .cjs,.ts .",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"build": "tsup index.ts --format cjs,esm --clean --dts-resolve",
		"test": "vitest run",
		"test:browser": "vitest run --browser.name=chrome --browser.headless --config ./vitest-browser.config.ts",
		"type-check": "tsc"
	},
	"author": "Hugging Face",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^18.13.0",
		"ts-node": "^10.9.1",
		"tsup": "^6.6.3",
		"typescript": "^4.9.5"
	},
	"dependencies": {}
}
